							"pm.test(\"Response validation\", function () {",
							"    pm.expect(pm.response.json()).to.have.property('id');",
							"    pm.expect(pm.response.json()).to.have.property('status');",
							"    pm.expect(pm.response.json()).to.have.property('basket_state');",
