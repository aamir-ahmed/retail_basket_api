            "name": "Create basket",
            "event": [
                {
                    "listen": "test",
                    "script": {
                        "id": "41971b8c-7f3c-4721-980d-6aaea9f18a0f",
                        "type": "text/javascript",
                        "exec": [
                            "pm.test(\"Status code is 200\", function () {",
                            "    pm.response.to.have.status(200);",
                            "});",
                            "",
                            "pm.test(\"Response validation\", function () {",
                            "    pm.expect(pm.response.json()).to.have.property('id');",
                            "    pm.expect(pm.response.json()).to.have.property('status');",
                            "    pm.expect(pm.response.json()).to.have.property('basket_state');",
                            "    pm.expect(pm.response.json().basket_state).to.eq('open');",
                            "});"
                        ]
                    }
                }
            ],
